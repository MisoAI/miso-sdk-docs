{
  "name": "@miso.ai/sdk-docs-monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "sites/client-sdk",
    "sites/server-sdk"
  ],
  "scripts": {
    "version": "node ./bin/version.js",
    "start:sites:client": "npm run start -w sites/client-sdk",
    "start:sites:client:preview": "npm run start:preview -w sites/client-sdk",
    "start:sites:server": "npm run start -w sites/server-sdk",
    "client": "npm run start:sites:client",
    "client:preview": "npm run start:sites:client:preview",
    "server": "npm run start:sites:server",
    "build:sites:client": "npm run build -w sites/client-sdk",
    "build:sites:server": "npm run build -w sites/server-sdk",
    "build:sites": "npm-run-all --parallel build:sites:*",
    "clean:index": "npm run clean:index --workspaces --if-present",
    "build:index": "npm run clean:index && node ./bin/build-index.js",
    "build": "npm run build:sites && npm run build:index"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.3.0",
    "fast-glob": "^3.3.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.2",
    "rollup": "^3.10.0",
    "sass": "^1.49.8"
  }
}
